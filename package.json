{
  "name": "angular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "lerna run start --scope=blog",
    "build": "npm run build:3:blog",
    "build:3:blog": "lerna run build --scope=blog",
    "build:2:service": "lerna run build --scope=service",
    "build:-1:service": "npm run build:2:service -- -- --watch",
    "build:1:shared": "lerna run build --scope=shared",
    "build:-1:shared": "npm run build:1:shared -- -- --watch",
    "build:2:tools": "lerna run build --scope=tools",
    "build:2:vendors": "lerna run build --scope=vendors",
    "watch": "lerna run watch --scope={vendors,shared,service,tools} --parallel",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@angular/service-worker": "^14.2.1",
    "emoji-toolkit": "^6.6.0",
    "firebase": "^9.9.4",
    "firebaseui": "^6.0.1",
    "firebaseui-angular": "^6.1.2",
    "marked": "^4.1.0",
    "ngx-markdown": "^14.0.1",
    "qrcode": "^1.5.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@types/jasmine": "~4.0.0",
    "@types/qrcode": "^1.5.0",
    "first-input-delay": "^0.1.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lerna": "^5.5.1",
    "ng-packagr": "^14.2.0",
    "nx": "^14.7.3",
    "typescript": "~4.7.2"
  },
  "workspaces": [
    "projects/*"
  ]
}
