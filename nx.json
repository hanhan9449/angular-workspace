{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build:*"]
      }
    },
    "targetDefaults": {
      "build:blog": {
        "dependOn": ["build:service", "build:vendors", "build:tools", "build:shared"]
      },
      "build:vendors": {
        "dependOn": ["build:shared"]
      },

      "build:tools": {
        "dependOn": ["build:shared"]
      }
    }
  }
}
